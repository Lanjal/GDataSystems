        <Spinner
            android:id="@+id/rgMag"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="#ffeecc"
            android:entries="@array/mag1"
            app:layout_constraintLeft_toRightOf="@+id/tvmag"
            app:layout_constraintTop_toBottomOf="@+id/tvtipoamostra" />

        <TextView
            android:id="@+id/tvveg"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/veg"
            android:textColor="#aa2222"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault.Large"
            app:layout_constraintLeft_toRightOf="@+id/rgMag"
            app:layout_constraintTop_toBottomOf="@+id/tvtipoamostra" />

        <Spinner
            android:id="@+id/rgTextVeg"
            android:layout_width="170dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="#ffeecc"
            android:entries="@array/veg"
            app:layout_constraintLeft_toRightOf="@+id/tvveg"
            app:layout_constraintTop_toBottomOf="@+id/rgTipoamostra" />

        <TextView
            android:id="@+id/tvpeso"
            android:layout_width="110dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/peso"
            android:textColor="#aa2222"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault.Large"
