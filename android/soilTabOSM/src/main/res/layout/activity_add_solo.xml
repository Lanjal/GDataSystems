        <Spinner
            android:id="@+id/rgTipoamostra"
            android:layout_width="0dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="#ffeecc"
            android:entries="@array/tipoamostra1"
            app:layout_constraintLeft_toRightOf="@+id/tvtipoamostra"
            app:layout_constraintTop_toBottomOf="@+id/tvns" />

        <TextView
            android:id="@+id/tvgranul"
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/granul"
            android:textColor="#aa2222"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault.Large"
            app:layout_constraintLeft_toRightOf="@id/rgTipoamostra"
