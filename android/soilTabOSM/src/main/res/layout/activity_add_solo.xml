        <Spinner
            android:id="@+id/rgGranul"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="#ffeecc"
            android:entries="@array/granul1"
            app:layout_constraintLeft_toRightOf="@+id/tvgranul"
            app:layout_constraintTop_toBottomOf="@+id/tvtipoperfil" />

        <TextView
            android:id="@+id/tvrelevo"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/relevo"
            android:textColor="#aa2222"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault.Large"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvprofm" />

        <Spinner
            android:id="@+id/rgRelevo"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="#ffeecc"
            android:entries="@array/relevo1"
            app:layout_constraintLeft_toRightOf="@+id/tvrelevo"
            app:layout_constraintTop_toBottomOf="@+id/textProfm" />

        <TextView
            android:id="@+id/tvfrag"
            android:layout_width="110dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/frag"
            android:textColor="#aa2222"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault.Large"
            app:layout_constraintLeft_toRightOf="@id/rgRelevo"
            app:layout_constraintTop_toBottomOf="@+id/textProfm" />

        <Spinner
            android:id="@+id/rgFrag"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:background="#ffeecc"
            android:entries="@array/frag1"
            app:layout_constraintLeft_toRightOf="@+id/tvfrag"
            app:layout_constraintTop_toBottomOf="@+id/tvcor" />

        <TextView
            android:id="@+id/tvmag"
            android:layout_width="120dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/mag"
            android:textColor="#aa2222"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault.Large"
